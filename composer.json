{
	"name": "nielslange/smntcs-google-webmaster-tools",
	"description": "Adds the verification code of Google Search Console, former Google Webmaster Tools, to your site.",
	"homepage": "https://github.com/nielslange/smntcs-google-webmaster-tools",
	"support": {
		"issues": "https://github.com/nielslange/smntcs-google-webmaster-tools/issues",
		"source": "https://github.com/nielslange/smntcs-google-webmaster-tools"
	},
	"type": "wordpress-plugin",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Private Site"
	],
	"license": "GPL-3.0-or-later",
	"authors": [
		{
			"name": "Niels Lange",
			"email": "info@nielslange.de"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"humanmade/psalm-plugin-wordpress": "^2.0",
		"overtrue/phplint": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3.6",
		"vimeo/psalm": "^4.7",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"scripts": {
		"cbf": "phpcbf --standard=.phpcs.xml .",
		"cs": "phpcs --standard=.phpcs.xml .",
		"lint": "phplint",
		"psalm": "psalm --no-cache"
	}
}
